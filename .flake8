[flake8]
max-line-length = 127
max-complexity = 10
exclude = 
    .git,
    __pycache__,
    .pytest_cache,
    venv,
    env,
    .venv,
    .env,
    migrations,
    models/__pycache__,
    data/__pycache__
ignore = 
    E203,
    W503,
    E501,
    F401,
    F541,
    F601,
    F841,
    C901
per-file-ignores =
    __init__.py:F401
